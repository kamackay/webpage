{
  "name": "webpage",
  "version": "1.0.0",
  "homepage": "https://keithmackay.com",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "npm-run-all build:*",
    "build:css": "node-sass src/ -o src/ ",
    "build:js": "react-scripts-ts build",
    "eject": "react-scripts-ts eject",
    "format": "prettier --write --check src/**/*.ts src/**/*.tsx src/**/*.js src/**/*.jsx src/**/*.css",
    "go": "yarn build && serve build/",
    "start": "yarn && npm-run-all -p watch:css start:js",
    "start:js": "react-scripts-ts start",
    "test": "react-scripts-ts test --env=jsdom",
    "watch:css": "npm run build:css && node-sass src/ -o src/ -w -r"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@material-ui/core": "4.5.2",
    "@material-ui/icons": "^4.5.1",
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.2.7",
    "@types/d3": "^5.5.0",
    "@types/jquery": "^3.3.27",
    "@types/node": "^12.0.2",
    "@types/qrcode": "^1.3.3",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.3",
    "@types/react-scroll": "^1.5.4",
    "@types/react-spinkit": "^3.0.5",
    "@types/smooth-scroll": "^16.1.0",
    "@types/toastr": "^2.1.37",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "d3": "^5.9.2",
    "jquery": "^3.4.1",
    "node-sass": "^4.12.0",
    "qrcode": "^1.4.1",
    "react": "^16.8.6",
    "react-device-detect": "^1.11.14",
    "react-dom": "^16.8.6",
    "react-https-redirect": "^1.1.0",
    "react-router-dom": "^5.0.0",
    "react-scripts-ts": "^3.1.0",
    "react-scroll": "^1.7.11",
    "react-spinkit": "^3.0.0",
    "react-spinners": "^0.5.4",
    "semantic-ui-react": "^0.87.2",
    "smooth-scroll": "^16.1.0",
    "toastr": "^2.1.4",
    "tslib": "^1.10.0",
    "typescript-optional": "^2.0.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "eslint": "^6.6.0",
    "npm-run-all": "^4.1.5",
    "serve": "^11.0.1",
    "typescript": "^3.4.5",
    "yarn": "^1.16.0"
  }
}
